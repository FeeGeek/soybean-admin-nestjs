{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "infra/casbin/config/*.conf",
        "outDir": "./dist/src"
      },
      {
        "include": "**/*.yaml",
        "outDir": "./dist/src"
      }
    ]
  }
}
